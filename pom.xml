   <build>
      <defaultGoal>package</defaultGoal>
      <plugins>
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-war-plugin</artifactId>
            <version>2.1-beta-1</version>
            <configuration>
               <webappDirectory>target/laneweb</webappDirectory>
               <nonFilteredFileExtensions>
                  <nonFilteredFileExtension>ico</nonFilteredFileExtension>
                  <nonFilteredFileExtension>xmap</nonFilteredFileExtension>
               </nonFilteredFileExtensions>
               <webResources>
                  <resource>
                     <directory>src/main/webapp</directory>
                     <filtering>true</filtering>
                  </resource>
               </webResources>
            </configuration>
         </plugin>
         <plugin>
            <groupId>net.sf.alchim</groupId>
            <artifactId>yuicompressor-maven-plugin</artifactId>
            <version>0.7.1</version>
            <executions>
               <execution>
                  <goals>
                     <goal>compress</goal>
                  </goals>
               </execution>
            </executions>
            <configuration>
               <webappDirectory>target/laneweb</webappDirectory>
               <excludes>
                  <exclude>**/swfobject.js</exclude>
               </excludes>
               <aggregations>
                  <aggregation>
                     <removeIncluded>true</removeIncluded>
                     <output>target/laneweb/resources/css/lane-all.css</output>
                     <includes>
                        <include>layout-min.css</include>
                        <include>colors-min.css</include>
                        <include>shaded-border-min.css</include>
                        <include>som-nav-min.css</include>
                        <include>lane-nav-min.css</include>
                        <include>footer-min.css</include>
                        <include>search-form-min.css</include>
                        <include>section-menu-min.css</include>
                        <include>expandies-min.css</include>
                        <include>simple-module-min.css</include>
                        <include>narrow-module-min.css</include>
                        <include>content-module-min.css</include>
                        <include>main-module-min.css</include>
                        <include>table-styles-min.css</include>
                        <include>popups-min.css</include>
                        <include>breadcrumb-min.css</include>
                        <include>header-min.css</include>
                        <include>anchors-min.css</include>
                        <include>headers-min.css</include>
                        <include>lists-min.css</include>
                        <include>legend-dropdown-min.css</include>
                        <include>events-min.css</include>
                        <include>side-hd-module-min.css</include>
                        <include>online-resources-min.css</include>
                        <include>liaisons-min.css</include>
                        <include>autocomplete-min.css</include>
                        <include>tooltips-min.css</include>
                        <include>search-min.css</include>
                        <include>images-min.css</include>
                     </includes>
                  </aggregation>
                  <aggregation>
                     <removeIncluded>true</removeIncluded>
                     <output>target/laneweb/javascript/lane-all.js</output>
                     <includes>
                        <include>lane-min.js</include>
                        <include>accordionview-min.js</include>
                        <include>lane-expandies-min.js</include>
                        <include>lane-search-min.js</include>
                        <include>lane-tracking-min.js</include>
                        <include>lane-google-min.js</include>
                        <include>lane-webtrends-min.js</include>
                        <include>lane-popup-min.js</include>
                        <include>lane-metasearch-min.js</include>
                        <include>lane-ie-min.js</include>
                        <include>lane-search-facets-min.js</include>
                        <include>lane-search-facet-counts-min.js</include>
                        <include>lane-search-history-min.js</include>
                        <include>lane-tooltips-min.js</include>
                        <include>lane-spellcheck-min.js</include>
                        <include>lane-findit-min.js</include>
                        <include>lane-querymap-min.js</include>
                        <include>lane-popin-min.js</include>
                        <include>lane-popup-min.js</include>
                        <include>lane-ie-min.js</include>
                        <include>lane-teletype-min.js</include>
                        <include>lane-search-pico-min.js</include>
                        <include>lane-suggest-min.js</include>
                     </includes>
                  </aggregation>
               </aggregations>
            </configuration>
         </plugin>
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-site-plugin</artifactId>
            <version>2.1</version>
         </plugin>
      </plugins>
   </build>
   <reporting>
      <plugins>
         <plugin>
            <groupId>gr.abiss.mvn.plugins</groupId>
            <artifactId>maven-jstools-plugin</artifactId>
            <version>0.7</version>
            <inherited>false</inherited>
            <configuration>
               <jsDir>src/main/webapp/javascript</jsDir>
               <excludes>**/swfobject.js</excludes>
            </configuration>
            <reportSets>
               <reportSet>
                  <reports>
                     <report>jslint</report>
                     <report>jsdoc</report>
                  </reports>
               </reportSet>
            </reportSets>
         </plugin>
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-changes-plugin</artifactId>
            <configuration>
               <issueLinkTemplate>%URL%/default.php?%ISSUE%</issueLinkTemplate>
               <mailSender>
                  <name>Release Notification</name>
                  <email>ceyates@stanford.edu</email>
               </mailSender>
               <smtpHost>smtp.stanford.edu</smtpHost>
               <smtpPort implementation="java.lang.Integer">25</smtpPort>
               <toAddresses>
                  <toAddress implementation="java.lang.String">ceyates@stanford.edu</toAddress>
               </toAddresses>
            </configuration>
            <version>2.3</version>
            <reportSets>
               <reportSet>
                  <reports>
                     <report>changes-report</report>
                  </reports>
               </reportSet>
            </reportSets>
         </plugin>
      </plugins>
   </reporting>
