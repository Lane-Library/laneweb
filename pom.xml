    <build>
        <defaultGoal>package</defaultGoal>
        <plugins>
            <plugin>
            	<groupId>org.apache.maven.plugins</groupId>
            	<artifactId>maven-compiler-plugin</artifactId>
            	<version>2.3.2</version>
            	<configuration>
                    <target>1.6</target>
                    <source>1.6</source>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>cobertura-maven-plugin</artifactId>
                <version>2.4</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>clean</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <version>2.1.1</version>
                <configuration>
                    <webappDirectory>target/laneweb</webappDirectory>
                    <nonFilteredFileExtensions>
                        <nonFilteredFileExtension>ico</nonFilteredFileExtension>
                        <nonFilteredFileExtension>xmap</nonFilteredFileExtension>
                    </nonFilteredFileExtensions>
                    <webResources>
                        <resource>
                            <directory>src/main/webapp</directory>
                            <filtering>true</filtering>
                        </resource>
                    </webResources>
                </configuration>
            </plugin>
            <plugin>
                <groupId>net.alchim31.maven</groupId>
                <artifactId>yuicompressor-maven-plugin</artifactId>
                <version>1.1</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>compress</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <webappDirectory>target/laneweb</webappDirectory>
                    <excludes>
                        <exclude>**/swfobject.js</exclude>
                        <exclude>**/lane-all.css</exclude>
                        <exclude>**/lane-all.js</exclude>
                        <exclude>**/lane-mobile.css</exclude>
                        <exclude>**/lane-mobile.js</exclude>
                        <exclude>**/test/*/*.js</exclude>
                        <exclude>**/yui.js</exclude>
                        <exclude>**/yui.css</exclude>
                    </excludes>
                    <aggregations>
                        <aggregation>
                            <removeIncluded>true</removeIncluded>
                            <output>target/laneweb/resources/css/lane-all.css</output>
                            <includes>
                                <include>layout-min.css</include>
                                <include>bassett-min.css</include>
                                <include>som-nav-min.css</include>
                                <include>shaded-border-min.css</include>
                                <include>header-min.css</include>
                                <include>search-form-min.css</include>
                                <include>lane-nav-min.css</include>
                                <include>lists-min.css</include>
                                <include>anchors-min.css</include>
                                <include>footer-min.css</include>
                                <include>headers-min.css</include>
                                <include>module-min.css</include>
                                <include>autocomplete-min.css</include>
                                <include>search-min.css</include>
                                <include>popups-min.css</include>
                                <include>online-resources-min.css</include>
                                <include>tooltips-min.css</include>
                                <include>miscellaneous-min.css</include>
                                <include>events-min.css</include>
                                <include>node-accordion-min.css</include>
                                <include>expandies-min.css</include>
                                <include>buttons-min.css</include>
                                <include>breadcrumb-form-min.css</include>
                                <include>tutorials-min.css</include>
                                <include>forms-min.css</include>
                                <include>section-menu-min.css</include>
                                <include>liaisons-min.css</include>
                                <include>table-styles-min.css</include>
                                <include>follow-min.css</include>
                                <include>images-min.css</include>
                            </includes>
                        </aggregation>
                        <aggregation>
                            <removeIncluded>true</removeIncluded>
                            <output>target/laneweb/resources/javascript/lane-all.js</output>
                            <includes>
                                <include>lane-min.js</include>
                                <include>lane-ie-min.js</include>
                                <include>lane-mobile-ad-min.js</include>
                                <include>lane-search-result-min.js</include>
                                <include>lane-search-indicator-min.js</include>
                                <include>lane-bassett-min.js</include>
                                <include>lane-textinputs-min.js</include>
                                <include>lane-suggest-min.js</include>
                                <include>lane-search-pico-min.js</include>
                                <include>lane-search-min.js</include>
                                <include>lane-tracking-min.js</include>
                                <include>lane-expandies-min.js</include>
                                <include>lane-google-min.js</include>
                                <include>lane-webtrends-min.js</include>
                                <include>lane-popup-min.js</include>
                                <include>lane-metasearch-min.js</include>
                                <include>lane-search-facets-min.js</include>
                                <include>lane-search-facet-counts-min.js</include>
                                <include>lane-search-history-min.js</include>
                                <include>lane-tooltips-min.js</include>
                                <include>lane-spellcheck-min.js</include>
                                <include>lane-findit-min.js</include>
                                <include>lane-querymap-min.js</include>
                                <include>lane-teletype-min.js</include>
                                <include>lane-selections-min.js</include>
                                <include>lane-popin-min.js</include>
                                <include>lane-quicklinks-min.js</include>
                                <include>lane-search-options-min.js</include>
                                <!--<include>lane-faq-expandies-min.js</include>-->
                                <include>lane-forms-min.js</include>
                                <include>lane-search-reset-min.js</include>
                                <include>lane-search-hoverText-min.js</include>
                                <!--  pending solution to expandies falling off page
                        <include>lane-column-height-min.js</include>
                         -->
                            </includes>
                        </aggregation>
                        <aggregation>
                            <removeIncluded>true</removeIncluded>
                            <output>target/laneweb/resources/mobile/css/lane-mobile.css</output>
                            <includes>
                                <include>m-iui-min.css</include>
                                <include>m-iui-default-theme-min.css</include>
                                <include>m-lane-min.css</include>
                            </includes>
                        </aggregation>
                        <aggregation>
                            <removeIncluded>true</removeIncluded>
                            <output>target/laneweb/resources/mobile/javascript/lane-mobile.js</output>
                            <includes>
                                <include>m-iui-min.js</include>
                                <include>m-lane-min.js</include>
                                <include>m-suggest-min.js</include>
                                <include>m-tracking-min.js</include>
                            </includes>
                        </aggregation>
                    </aggregations>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-site-plugin</artifactId>
                <version>3.0-beta-3</version>
            </plugin>
        </plugins>
    </build>
