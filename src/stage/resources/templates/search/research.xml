    <body>
	<br/>
        <div id="lw_alt-title"><img src="/images/icons/helix-tiny.gif" alt="" align="middle"/>Bioresearch</div>
        <div class="searchHeader">
 			<table id="searchHeaderTable" width="100%" cellspacing="0">
				<tr>
					<td class="searchHeaderLeft"><span class="searchHeaderTitle">Bioresearch Search</span></td>
					<td id="incrementalResultsProgressBar" class="searchHeaderMiddle"><span style="margin-right:200px;">Search starting ... </span></td>
					<td id="incrementalResultsDetails" class="searchHeaderRight"> <!--  original spec put final details here ... not currenty used--></td>
				</tr>
				<tr>
					<td class="popInContent" colspan="3">
				        <span id="spellResults"> Run query in: <a href="search.html?source=clinical&amp;id={$id}">Clinical</a> 
						or <a href="search.html?source=all&amp;id={$id}"> LaneConnex</a>
						| More resources: <a href="portals/bioresearch.html">Bioresearch Portal</a></span>
					</td>
				</tr>
			</table>
        </div>
	<!--  carfeul about moving elements here ... search.js looks for ordered elements, not named -->
	
            <div id="incrementalSearchResults" class="hide">
			
	            <div class="searchColumn3 largeFont">

				<div class="searchCategory">
