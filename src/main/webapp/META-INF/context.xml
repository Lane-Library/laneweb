<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <Resource name="jdbc/eresources" auth="Container"
        type="javax.sql.DataSource"
        driverClassName="oracle.jdbc.driver.OracleDriver"
        url="${laneweb.context.eresources.url}"
        username="${laneweb.context.eresources.username}"
        password="${laneweb.context.eresources.password}"
        maxActive="${laneweb.context.eresources.maxActive}"
        maxIdle="${laneweb.context.eresources.maxIdle}"
        maxWait="${laneweb.context.eresources.maxWait}"/>
    <Resource name="jdbc/voyager" auth="Container"
        type="javax.sql.DataSource"
        driverClassName="oracle.jdbc.driver.OracleDriver"
        url="${laneweb.context.voyager.url}"
        username="${laneweb.context.voyager.username}"
        password="${laneweb.context.voyager.password}"
        maxActive="${laneweb.context.voyager.maxActive}"
        maxIdle="${laneweb.context.voyager.maxIdle}"
        maxWait="${laneweb.context.voyager.maxWait}"
        validationQuery="SELECT 1 FROM DUAL"/>
    <Environment name="live-base"
        type="java.lang.String"
        value="${laneweb.context.environment.live-base}"/>
    <Environment name="stage-base"
        type="java.lang.String"
        value="${laneweb.context.environment.stage-base}"/>
    <Environment name="medblog-base"
        type="java.lang.String"
        value="${laneweb.context.environment.medblog-base}"/>
    <Environment name="ezproxy-key"
        type="java.lang.String"
        value="${laneweb.context.environment.ezproxy-key}"/>
    <Environment name="webdash-key"
        type="java.lang.String"
        value="${laneweb.context.environment.webdash-key}"/>
    <Environment name="google-key"
        type="java.lang.String"
        value="${laneweb.context.environment.google-key}"/>
    <Environment name="metasearch-failed"
        type="java.lang.String"
        value="${laneweb.context.environment.metasearch-failed}"/>
</Context>
