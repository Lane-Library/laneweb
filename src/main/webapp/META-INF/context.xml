<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <Resource name="jdbc/eresources" auth="Container"
        description="eresources - ${edu.stanford.irt.laneweb.db.eresources.user} - jdbc/eresources"
        connectionPoolName = "eresources - ${edu.stanford.irt.laneweb.db.eresources.user} - Connection Pool"
        dataSourceName = "eresources - ${edu.stanford.irt.laneweb.db.eresources.user} - Data Source"
        type="oracle.ucp.jdbc.PoolDataSource"
        factory="oracle.ucp.jdbc.PoolDataSourceImpl"
        connectionFactoryClassName="oracle.jdbc.pool.OracleDataSource"
        url="${edu.stanford.irt.laneweb.db.eresources.url}"
        user="${edu.stanford.irt.laneweb.db.eresources.user}"
        password="${edu.stanford.irt.laneweb.db.eresources.password}"
        onsConfiguration="${edu.stanford.irt.laneweb.db.eresources.onsConfiguration}"
        fastConnectionFailoverEnabled="${edu.stanford.irt.laneweb.db.eresources.fastConnectionFailoverEnabled}"
        initialPoolSize = "${edu.stanford.irt.laneweb.db.eresources.initialPoolSize}"
        minPoolSize = "${edu.stanford.irt.laneweb.db.eresources.minPoolSize}"
        maxPoolSize = "${edu.stanford.irt.laneweb.db.eresources.maxPoolSize}"
    />
    <Resource name="jdbc/voyager" auth="Container"
        description="voyager - ${edu.stanford.irt.laneweb.db.voyager.user} - jdbc/voyager"
        connectionPoolName = "voyager - ${edu.stanford.irt.laneweb.db.voyager.user} - Connection Pool"
        dataSourceName = "voyager - ${edu.stanford.irt.laneweb.db.voyager.user} - Data Source"
        type="oracle.ucp.jdbc.PoolDataSource"
        factory="oracle.ucp.jdbc.PoolDataSourceImpl"
        connectionFactoryClassName="oracle.jdbc.pool.OracleDataSource"
        url="${edu.stanford.irt.laneweb.db.voyager.url}"
        user="${edu.stanford.irt.laneweb.db.voyager.user}"
        password="${edu.stanford.irt.laneweb.db.voyager.password}"
        initialPoolSize = "${edu.stanford.irt.laneweb.db.voyager.initialPoolSize}"
        minPoolSize = "${edu.stanford.irt.laneweb.db.voyager.minPoolSize}"
        maxPoolSize = "${edu.stanford.irt.laneweb.db.voyager.maxPoolSize}"
    />
    <Resource name="jdbc/grandrounds" auth="Container"
        description="grandrounds - ${edu.stanford.irt.laneweb.db.grandrounds.user} - jdbc/grandrounds"
        connectionPoolName = "grandrounds - ${edu.stanford.irt.laneweb.db.grandrounds.user} - Connection Pool"
        dataSourceName = "grandrounds - ${edu.stanford.irt.laneweb.db.grandrounds.user} - Data Source"
        type="oracle.ucp.jdbc.PoolDataSource"
        factory="oracle.ucp.jdbc.PoolDataSourceImpl"
        connectionFactoryClassName="oracle.jdbc.pool.OracleDataSource"
        url="${edu.stanford.irt.laneweb.db.grandrounds.url}"
        user="${edu.stanford.irt.laneweb.db.grandrounds.user}"
        password="${edu.stanford.irt.laneweb.db.grandrounds.password}"
        initialPoolSize = "${edu.stanford.irt.laneweb.db.grandrounds.initialPoolSize}"
        minPoolSize = "${edu.stanford.irt.laneweb.db.grandrounds.minPoolSize}"
        maxPoolSize = "${edu.stanford.irt.laneweb.db.grandrounds.maxPoolSize}"
    />
    <Environment name="metasearch-failed"
        type="java.lang.String"
        value="${edu.stanford.irt.laneweb.metasearch-failed}"/>
</Context>
