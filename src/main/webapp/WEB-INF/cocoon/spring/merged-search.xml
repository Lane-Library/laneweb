<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean name="org.apache.cocoon.generation.Generator/merged-search"
        class="edu.stanford.irt.laneweb.searchresults.MergedSearchGenerator"
        parent="edu.stanford.irt.laneweb.search.AbstractSearchGenerator">
        <property name="collectionManager"
            ref="edu.stanford.irt.eresources.CollectionManager/laneconnex"/>
        <property name="defaultMetasearchTimeout" value="20000"/>
        <property name="contentResultLimit" value="10"/>
    </bean>

</beans>
