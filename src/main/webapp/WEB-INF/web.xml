            /WEB-INF/spring/merged-search.xml
            /WEB-INF/spring/model.xml
            /WEB-INF/spring/pipeline.xml
            /WEB-INF/spring/proxy.xml
            /WEB-INF/spring/querymap.xml
            /WEB-INF/spring/search.xml
            /WEB-INF/spring/servlet.xml
            /WEB-INF/spring/sitemap.xml
            /WEB-INF/spring/suggest.xml
        </param-value>
    </context-param>

    <context-param>
        <param-name>laneweb.context.version</param-name>
        <param-value>${project.version}</param-value>
    </context-param>

    <listener>
        <listener-class>edu.stanford.irt.laneweb.servlet.LanewebContextListener</listener-class>
    </listener>

    <listener>
        <listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
    </listener>

    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>

    <listener>
        <listener-class>org.springframework.web.context.request.RequestContextListener</listener-class>
    </listener>
    
    <listener>
        <listener-class>net.bull.javamelody.SessionListener</listener-class>
    </listener>
    
    <filter>
        <filter-name>monitoring</filter-name>
        <filter-class>net.bull.javamelody.MonitoringFilter</filter-class>
        <init-param>
            <param-name>displayed-counters</param-name>
            <param-value>http,sql,error,log,spring</param-value>
        </init-param>
    </filter>
    
    <filter>
        <filter-name>LanewebHTTPSFilter</filter-name>
        <filter-class>edu.stanford.irt.laneweb.servlet.LanewebHTTPSFilter</filter-class>
    </filter>
    
    <filter-mapping>
        <filter-name>monitoring</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>LanewebHTTPSFilter</filter-name>
        <url-pattern>/secure/*</url-pattern>
    </filter-mapping>
    
    <servlet>
        <description>ModelAugmentingRequestHandler</description>
        <display-name>ModelAugmentingRequestHandler</display-name>
        <servlet-name>edu.stanford.irt.laneweb.servlet.ModelAugmentingRequestHandler</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    
    <servlet>
        <description>MobileRequestHandler</description>
        <display-name>ModelAugmentingRequestHandler</display-name>
        <servlet-name>edu.stanford.irt.laneweb.servlet.MobileRequestHandler</servlet-name>
        <servlet-class>org.springframework.web.context.support.HttpRequestHandlerServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <description>LogoutServlet</description>
        <display-name>LogoutServlet</display-name>
        <servlet-name>edu.stanford.irt.laneweb.servlet.LogoutServlet</servlet-name>
        <servlet-class>edu.stanford.irt.laneweb.servlet.LogoutServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>edu.stanford.irt.laneweb.servlet.ModelAugmentingRequestHandler</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>edu.stanford.irt.laneweb.servlet.MobileRequestHandler</servlet-name>
        <url-pattern>/m/*</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>edu.stanford.irt.laneweb.servlet.MobileRequestHandler</servlet-name>
        <url-pattern>/mobile/*</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>edu.stanford.irt.laneweb.servlet.MobileRequestHandler</servlet-name>
        <url-pattern>/ms/*</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>edu.stanford.irt.laneweb.servlet.LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

