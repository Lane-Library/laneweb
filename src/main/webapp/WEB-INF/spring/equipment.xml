<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/equipment"
        class="edu.stanford.irt.laneweb.equipment.EquipmentGenerator"
        scope="prototype">
        <constructor-arg ref="javax.sql.DataSource/grandrounds"/>
        <constructor-arg value="classpath:/edu/stanford/irt/laneweb/equipment/getEquipment.fnc"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Transformer/equipment-status"
        class="edu.stanford.irt.laneweb.equipment.EquipmentStatusTransformer"
        scope="prototype">
        <constructor-arg ref="javax.sql.DataSource/grandrounds"></constructor-arg>
    </bean>
    
</beans>