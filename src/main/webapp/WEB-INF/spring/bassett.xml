<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/bassett"
        class="edu.stanford.irt.laneweb.bassett.BassettImageGenerator"
        scope="prototype">
        <constructor-arg ref="edu.stanford.irt.solr.service"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/bassett-list"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/bassett-accordion"
        class="edu.stanford.irt.laneweb.bassett.BassettAccordionGenerator"
        scope="prototype">
        <constructor-arg ref="edu.stanford.irt.solr.service"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/bassett-count"/>
    </bean>
    
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/bassett-list"
        class="edu.stanford.irt.laneweb.bassett.BassettImageListSAXStrategy"/>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/bassett-count"
        class="edu.stanford.irt.laneweb.bassett.BassettCountSAXStrategy"/>
    
</beans>
