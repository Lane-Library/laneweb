<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean name="org.apache.cocoon.generation.Generator/bassett"
        class="edu.stanford.irt.laneweb.bassett.BassettEresourcesGenerator"
        scope="prototype">
        <constructor-arg ref="edu.stanford.irt.laneweb.bassett.BassettCollectionManager"/>
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.bassett.BassettListSAXStrategy"/>
        </constructor-arg>
    </bean>
    
    <bean name="org.apache.cocoon.generation.Generator/bassett-accordion"
        class="edu.stanford.irt.laneweb.bassett.BassettAccordionEresourcesGenerator"
        scope="prototype">
        <constructor-arg ref="edu.stanford.irt.laneweb.bassett.BassettCollectionManager"/>
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.bassett.BassettCountSAXStrategy"/>
        </constructor-arg>
    </bean>
    
    <bean id="edu.stanford.irt.laneweb.bassett.BassettCollectionManager"
        class="edu.stanford.irt.laneweb.bassett.BassettCollectionManager">
        <constructor-arg ref="javax.sql.DataSource/eresources"/>
    </bean>
    
</beans>
