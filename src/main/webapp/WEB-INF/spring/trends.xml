<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:task="http://www.springframework.org/schema/task"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
                       http://www.springframework.org/schema/task http://www.springframework.org/schema/task/spring-task.xsd">

    <task:annotation-driven/>
    
    <bean name="edu.stanford.irt.laneweb.trends.BookmarkTrendsReporter" 
        class="edu.stanford.irt.laneweb.trends.BookmarkTrendsReporter">
        <property name="googleTracker" ref="edu.stanford.irt.laneweb.trends.GoogleTracker"/>
        <property name="bookmarkDAO" ref="edu.stanford.irt.laneweb.bookmarks.BookmarkDAO"/>
    </bean>
    
    <bean class="edu.stanford.irt.laneweb.trends.GoogleTracker"
        name="edu.stanford.irt.laneweb.trends.GoogleTracker">
        <property name="domainName">
            <value>lane.stanford.edu</value>
        </property>
        <property name="referer">
            <value>http://lane.stanford.edu/index.html</value>
        </property>
        <property name="googleAccount">
            <value>UA-3202241-12</value>
        </property>
        <property name="userAgent">
            <value>laneweb</value>
        </property>
    </bean>


</beans>
