<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
    
    <bean name="edu.stanford.irt.laneweb.voyager.VoyagerLogin/lmldb"
        class="edu.stanford.irt.laneweb.voyager.VoyagerLogin">
        <constructor-arg ref="org.slf4j.Logger/VoyagerLogin"/>
        <property name="voyagerDatabase" value="lmldb"/>
        <property name="errorURL" value="/voyagerError.html"/>
        <property name="dataSource" ref="javax.sql.DataSource/voyager"/>
    </bean>
    
    <bean name="edu.stanford.irt.laneweb.voyager.VoyagerLogin/jbldb"
        class="edu.stanford.irt.laneweb.voyager.VoyagerLogin">
        <constructor-arg ref="org.slf4j.Logger/VoyagerLogin"/>
        <property name="voyagerDatabase" value="jbldb"/>
        <property name="errorURL" value="http://jbldb.stanford.edu/login-error.html"/>
        <property name="dataSource" ref="javax.sql.DataSource/voyager"/>
    </bean>

</beans>
