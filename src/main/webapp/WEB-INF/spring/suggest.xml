<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">
    
    
    <bean name="edu.stanford.irt.suggest.SuggestionManager/eresource"
        class="edu.stanford.irt.laneweb.suggest.SolrSuggestionManager">
        <constructor-arg ref="edu.stanford.irt.laneweb.eresources.CollectionManager/laneconnex"/>
    </bean>
    
    <bean name="edu.stanford.irt.suggest.SuggestionManager/mesh"
        class="edu.stanford.irt.suggest.MeshSuggestionManager"/>
    
    <bean name="edu.stanford.irt.suggest.SuggestionManager/extensions-suggest"
        class="edu.stanford.irt.laneweb.suggest.CompositeSuggestionManager">
        <constructor-arg>
            <list>
                <ref bean="edu.stanford.irt.suggest.SuggestionManager/eresource"/>
                <ref bean="edu.stanford.irt.suggest.SuggestionManager/mesh"/>
            </list>
        </constructor-arg>
    </bean>
    
</beans>
