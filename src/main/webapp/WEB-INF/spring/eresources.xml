<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean name="edu.stanford.irt.eresources.CollectionManager/laneconnex"
        class="edu.stanford.irt.laneweb.eresources.EresourcesCollectionManager">
        <constructor-arg ref="javax.sql.DataSource/eresources"/>
        <constructor-arg>
            <bean class="org.springframework.beans.factory.config.PropertiesFactoryBean">
                <property name="location" value="classpath:edu/stanford/irt/laneweb/eresources/eresources-sql.properties"/>
            </bean>
        </constructor-arg>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/eresource"
        class="edu.stanford.irt.laneweb.eresources.EresourceSAXStrategy"/>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource"
        class="edu.stanford.irt.laneweb.eresources.PagingEresourceListSAXStrategy">
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/eresource"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource-xhtml"
        class="edu.stanford.irt.laneweb.eresources.PagingEresourceListXHTMLSAXStrategy">
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.eresources.EresourceXHTMLSAXStrategy"/>
        </constructor-arg>
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.eresources.PagingDataSAXStrategy"/>
        </constructor-arg>
    </bean>

    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/eresources-count"
        class="edu.stanford.irt.laneweb.eresources.EresourcesCountGenerator"
        scope="prototype">
        <constructor-arg>
            <set>
                <value>ej</value>
                <value>database</value>
                <value>graphic</value>
                <value>video</value>
                <value>book</value>
                <value>cc</value>
                <value>lanesite</value>
                <value>software</value>
                <value>statistics</value>
            </set>
        </constructor-arg>
        <constructor-arg ref="edu.stanford.irt.eresources.CollectionManager/laneconnex"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-browse"
        class="edu.stanford.irt.laneweb.eresources.BrowseEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-browse"/>
        <constructor-arg ref="edu.stanford.irt.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-browse-xhtml"
        class="edu.stanford.irt.laneweb.eresources.BrowseEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-browse-xhtml"/>
        <constructor-arg ref="edu.stanford.irt.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource-xhtml"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-core"
        class="edu.stanford.irt.laneweb.eresources.CoreEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-core"/>
        <constructor-arg ref="edu.stanford.irt.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-core-xhtml"
        class="edu.stanford.irt.laneweb.eresources.CoreEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-core-xhtml"/>
        <constructor-arg ref="edu.stanford.irt.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource-xhtml"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-mesh"
        class="edu.stanford.irt.laneweb.eresources.MeSHEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-mesh"/>
        <constructor-arg ref="edu.stanford.irt.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-mesh-xhtml"
        class="edu.stanford.irt.laneweb.eresources.MeSHEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-mesh-xhtml"/>
        <constructor-arg ref="edu.stanford.irt.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource-xhtml"/>
    </bean>

</beans>
