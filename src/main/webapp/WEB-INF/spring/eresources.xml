<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">
    
    <bean name="edu.stanford.irt.laneweb.eresources.CollectionManager/laneconnex"
        class="edu.stanford.irt.laneweb.solr.SolrSearchService"/>
    
     <bean name="edu.stanford.irt.laneweb.solr.SolrQueryParser"
        class="edu.stanford.irt.laneweb.solr.SolrQueryParser">
        <constructor-arg>
            <list>
                <bean class="edu.stanford.irt.laneweb.solr.AdvancedQueryInspector"/>
                <bean class="edu.stanford.irt.laneweb.solr.PmidQueryInspector"/>
                <bean class="edu.stanford.irt.laneweb.solr.NumberQueryInspector"/>
                <bean class="edu.stanford.irt.laneweb.solr.EscapingQueryInspector"/>
            </list>
        </constructor-arg>
     </bean>
     
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/eresource-xml"
        class="edu.stanford.irt.laneweb.eresources.EresourceSAXStrategy"/>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource-html"
        class="edu.stanford.irt.laneweb.eresources.PagingEresourceListXHTMLSAXStrategy">
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.eresources.EresourceXHTMLSAXStrategy"/>
        </constructor-arg>
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.eresources.EresourceListPagingDataSAXStrategy"/>
        </constructor-arg>
    </bean>

    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/eresources-count"
        class="edu.stanford.irt.laneweb.eresources.EresourcesCountGenerator"
        scope="prototype">
        <constructor-arg>
            <set>
                <value>article</value>
                <value>audio</value>
                <value>book</value>
                <value>bookdigital</value>
                <value>bookprint</value>
                <value>catalog</value>
                <value>cc</value>
                <value>database</value>
                <value>image</value>
                <value>journal</value>
                <value>journaldigital</value>
                <value>journalprint</value>
                <value>laneclass</value>
                <value>lanepage</value>
                <value>lanesite</value>
                <value>other</value>
                <value>software</value>
                <value>statistics</value>
                <value>video</value>
                <value>website</value>
            </set>
        </constructor-arg>
        <constructor-arg ref="edu.stanford.irt.laneweb.eresources.CollectionManager/laneconnex"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-browse-html"
        class="edu.stanford.irt.laneweb.eresources.BrowseEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-browse-html"/>
        <constructor-arg ref="edu.stanford.irt.laneweb.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource-html"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-core-html"
        class="edu.stanford.irt.laneweb.eresources.CoreEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-core-html"/>
        <constructor-arg ref="edu.stanford.irt.laneweb.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource-html"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/er-mesh-html"
        class="edu.stanford.irt.laneweb.eresources.MeSHEresourcesGenerator"
        scope="prototype">
        <constructor-arg value="er-mesh-html"/>
        <constructor-arg ref="edu.stanford.irt.laneweb.eresources.CollectionManager/laneconnex"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/paging-eresource-html"/>
    </bean>

</beans>
