<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="org.apache.cocoon.el.objectmodel.ObjectModel"
        class="edu.stanford.irt.laneweb.cocoon.expression.LanewebObjectModel">
    </bean>
    
    <bean name="org.apache.cocoon.transformation.Transformer/debug"
        class="edu.stanford.irt.laneweb.cocoon.DebugTransformer"
        scope="prototype"/>
    
    <bean id="org.apache.cocoon.environment.Environment"
        class="edu.stanford.irt.laneweb.cocoon.LanewebEnvironment"
        scope="request">
    </bean>
    
</beans>
