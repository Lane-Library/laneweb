<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean id="com.fasterxml.jackson.databind.ObjectMapper" class="edu.stanford.irt.laneweb.mapping.LanewebObjectMapper" />
    
    <bean id="jacksonSerializationConfig" class="com.fasterxml.jackson.databind.SerializationConfig"
        factory-bean="com.fasterxml.jackson.databind.ObjectMapper" factory-method="getSerializationConfig" >
    </bean>
    
</beans>
