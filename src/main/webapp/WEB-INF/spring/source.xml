<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean name="org.apache.excalibur.source.SourceFactory/content"
        class="edu.stanford.irt.laneweb.cocoon.source.LanewebSitemapSourceFactory">
        <property name="processor" ref="org.apache.cocoon.Processor/content"/>
    </bean>
    
    <bean name="org.apache.excalibur.source.SourceFactory/eresources"
        class="edu.stanford.irt.laneweb.cocoon.source.LanewebSitemapSourceFactory">
        <property name="processor" ref="org.apache.cocoon.Processor/eresources"/>
    </bean>
    
    <bean name="org.apache.excalibur.source.SourceFactory/applications"
        class="edu.stanford.irt.laneweb.cocoon.source.LanewebSitemapSourceFactory">
        <property name="processor" ref="org.apache.cocoon.Processor/applications"/>
    </bean>
    
    <bean name="org.apache.excalibur.source.SourceFactory/rss"
        class="edu.stanford.irt.laneweb.cocoon.source.LanewebSitemapSourceFactory">
        <property name="processor" ref="org.apache.cocoon.Processor/rss"/>
    </bean>
    
    <bean name="org.apache.excalibur.source.SourceFactory/mobile"
        class="edu.stanford.irt.laneweb.cocoon.source.LanewebSitemapSourceFactory">
        <property name="processor" ref="org.apache.cocoon.Processor/mobile"/>
    </bean>
    
    <bean id="org.apache.excalibur.source.SourceResolver"
        class="edu.stanford.irt.laneweb.cocoon.source.LanewebSourceResolver">
        <property name="servletContext" ref="servletContext"/>
        <property name="sourceFactories">
            <map>
                <entry key="content" value-ref="org.apache.excalibur.source.SourceFactory/content"/>
                <entry key="apps" value-ref="org.apache.excalibur.source.SourceFactory/applications"/>
                <entry key="eresources" value-ref="org.apache.excalibur.source.SourceFactory/eresources"/>
                <entry key="rss" value-ref="org.apache.excalibur.source.SourceFactory/rss"/>
                <entry key="mobile" value-ref="org.apache.excalibur.source.SourceFactory/mobile"/>
            </map>
        </property>
    </bean>
    
    <bean class="org.springframework.beans.factory.config.CustomEditorConfigurer">
        <property name="propertyEditorRegistrars">
            <list>
                <bean class="edu.stanford.irt.laneweb.cocoon.source.SourceEditorRegistrar"/>
            </list>
        </property>
    </bean>

</beans>
