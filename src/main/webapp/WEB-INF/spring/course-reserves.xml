<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/course-reserves-item-list"
        class="edu.stanford.irt.laneweb.coursereserves.CourseReservesItemListGenerator"
        scope="prototype">
        <constructor-arg ref="edu.stanford.irt.coursereserves.CourseReservesDAO"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/course-reserves-item-list"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/courses"
        class="edu.stanford.irt.laneweb.coursereserves.CourseListGenerator"
        scope="prototype">
        <constructor-arg ref="edu.stanford.irt.coursereserves.CourseReservesDAO"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/courses"/>
    </bean>
    
    <bean name="edu.stanford.irt.coursereserves.CourseReservesDAO"
        class="edu.stanford.irt.coursereserves.CourseReservesDAO">
        <constructor-arg ref="javax.sql.DataSource/grandrounds"/>
        <constructor-arg value="classpath:/edu/stanford/irt/coursereserves/itemStatus.sql"/>
        <constructor-arg value="classpath:/edu/stanford/irt/coursereserves/course.sql"/>
        <constructor-arg value="classpath:/edu/stanford/irt/coursereserves/courses.sql"/>
        <constructor-arg value="classpath:/edu/stanford/irt/coursereserves/courseReservesItemListAll.fnc"/>
        <constructor-arg value="classpath:/edu/stanford/irt/coursereserves/courseReservesItemListCourse.fnc"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/courses"
        class="edu.stanford.irt.laneweb.coursereserves.CoursesSAXStrategy"/>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/course-reserves-item-list"
        class="edu.stanford.irt.laneweb.coursereserves.CourseReservesItemListSAXStrategy">
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.coursereserves.CourseHeadingSAXStrategy"/>
        </constructor-arg>
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.coursereserves.CourseReservesItemSAXStrategy"/>
        </constructor-arg>
    </bean>
    
</beans>