<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd">
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/course-reserves-by-department"
        class="edu.stanford.irt.laneweb.coursereserves.CourseReservesDepartmentGenerator"
        scope="prototype">
        <constructor-arg value="classpath:/edu/stanford/irt/laneweb/coursereserves/by-department.sql"/>
        <constructor-arg ref="javax.sql.DataSource/grandrounds"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/course-reserves-resultset"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/course-reserves-by-title"
        class="edu.stanford.irt.laneweb.coursereserves.CourseReservesGenerator"
        scope="prototype">
        <constructor-arg value="classpath:/edu/stanford/irt/laneweb/coursereserves/by-title.sql"/>
        <constructor-arg ref="javax.sql.DataSource/grandrounds"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/departments-resultset"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/course-reserves-departments"
        class="edu.stanford.irt.laneweb.coursereserves.CourseReservesGenerator"
        scope="prototype">
        <constructor-arg value="classpath:/edu/stanford/irt/laneweb/coursereserves/departments.sql"/>
        <constructor-arg ref="javax.sql.DataSource/grandrounds"/>
        <constructor-arg ref="edu.stanford.irt.cocoon.xml.SAXStrategy/departments-resultset"/>
    </bean>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/course-reserves-resultset"
        class="edu.stanford.irt.laneweb.coursereserves.CourseReservesResultSetSAXStrategy"/>
    
    <bean name="edu.stanford.irt.cocoon.xml.SAXStrategy/departments-resultset"
        class="edu.stanford.irt.laneweb.coursereserves.DepartmentsResultSetSAXStrategy"/>
    
</beans>