<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
    
    <bean name="edu.stanford.irt.cocoon.pipeline.Generator/audio"
        class="edu.stanford.irt.laneweb.audio.AudioGenerator"
        scope="prototype">
        <constructor-arg ref="javax.sql.DataSource/grandrounds"/>
        <constructor-arg value="classpath:/edu/stanford/irt/laneweb/audio/getAudio.fnc"/>
        <constructor-arg>
            <bean class="edu.stanford.irt.laneweb.audio.RecordSAXStrategy"/>
        </constructor-arg>
    </bean>

</beans>
