<?xml version="1.0"?>
<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://apache.org/cocoon/sitemap/1.0 http://cocoon.apache.org/schema/sitemap/cocoon-sitemap-1.0.xsd">
    <map:pipelines>
        <map:pipeline type="noncaching">
            <map:match pattern="stage/**" type="wildcard">
                <map:mount uri-prefix="stage" check-reload="no" src="context://stage/stage.xmap"/>
            </map:match>
            <map:match pattern="ceyates/**" type="wildcard">
                <map:mount uri-prefix="ceyates" check-reload="yes"
                    src="file:/afs/ir.stanford.edu/users/c/e/ceyates/laneweb/laneweb.xmap"/>
            </map:match>
            <map:match pattern="ryanmax/**" type="wildcard">
                <map:mount uri-prefix="ryanmax" check-reload="yes"
                    src="file:/afs/ir.stanford.edu/users/r/y/ryanmax/laneweb/laneweb.xmap"/>
            </map:match>
            <map:match pattern="alainb/**" type="wildcard">
                <map:mount uri-prefix="alainb" check-reload="yes"
                    src="file:/afs/ir.stanford.edu/users/a/l/alainb/laneweb/laneweb.xmap"/>
            </map:match>
            <map:match pattern="olgary/**" type="wildcard">
                <map:mount uri-prefix="olgary" check-reload="yes"
                    src="file:/afs/ir.stanford.edu/users/o/l/olgary/laneweb/laneweb.xmap"/>
            </map:match>
            <map:match pattern="rzwies/**" type="wildcard">
                <map:mount uri-prefix="rzwies" check-reload="yes"
                    src="file:/afs/ir.stanford.edu/users/r/z/rzwies/laneweb/laneweb.xmap"/>
            </map:match>
            <map:match pattern="extensions/**" type="wildcard">
                <map:mount uri-prefix="extensions" check-reload="no" src="extensions.xmap"/>
            </map:match>
            <map:match pattern="tobacco/**" type="wildcard">
                <map:mount uri-prefix="tobacco" check-reload="no" src="tobacco.xmap"/>
            </map:match>
            <map:match pattern="robots.txt" type="wildcard">
                <map:read type="resource" src="robots.txt" mime-type="text/plain"/>
            </map:match>
            <map:match pattern="rss/**" type="wildcard">
                <map:mount uri-prefix="rss" check-reload="no" src="rss.xmap"/>
            </map:match>
            <map:match pattern="eresources/**" type="wildcard">
                <map:mount uri-prefix="eresources" check-reload="no" src="eresources.xmap"/>
            </map:match>
            <map:match pattern="content/**" type="wildcard">
                <map:mount uri-prefix="content" check-reload="no" src="content.xmap"/>
            </map:match>
            <map:match pattern="apps/**" type="wildcard">
                <map:mount uri-prefix="apps" check-reload="no" src="applications.xmap"/>
            </map:match>
            <map:match pattern="shc/**" type="wildcard">
                <map:mount uri-prefix="shc" check-reload="no" src="shc.xmap"/>
            </map:match>
            <map:match pattern="admin/status" type="wildcard">
                <map:generate type="status"/>
                <map:serialize type="xml"/>
            </map:match>
            <map:match pattern="admin/clearcache" type="wildcard">
                <map:act type="clear-cache"/>
                <map:generate type="status"/>
                <map:serialize type="xml"/>
            </map:match>
            <map:match pattern="secure/file/**" type="wildcard">
                <map:read type="resource" mime-type="text/plain" src="file:/{1}"/>
            </map:match>
            <map:match pattern="search/*" type="wildcard">
                <map:generate type="file" src="cocoon:/error.html"/>
                <map:serialize type="xhtml" status-code="503"/>
            </map:match>
            <map:match pattern="secure/login.html" type="wildcard">
                <map:act type="loginAction">
                    <map:parameter name="sunetid" value="${sunetid}"/>
                    <map:parameter name="ticket" value="${ticket}"/>
                    <map:parameter name="query-string" value="${query-string}"/>
                </map:act>
            </map:match>
            <map:match pattern="secure/webdash.html" type="wildcard">
                <map:act type="webdash">
                    <map:parameter name="nonce" value="${nonce}"/>
                    <map:parameter name="system-user-id"
                        value="${system_user_id}"/>
                    <map:parameter name="sunetid" value="${sunetid}"/>
                    <map:parameter name="name" value="${name}"/>
                    <map:parameter name="affiliation" value="${affiliation}"/>
                    <map:redirect-to uri="{webdash-url}"/>
                </map:act>
            </map:match>
            <map:match pattern="secure/voyager.html" type="wildcard">
                <map:act type="voyager">
                    <map:parameter name="pid" value="${PID}"/>
                    <map:parameter name="query-string" value="${query-string}"/>
                    <map:parameter name="univid" value="${univid}"/>
                    <map:redirect-to uri="{voyager-url}"/>
                </map:act>
            </map:match>
            <map:match pattern="redirect/cme" type="wildcard">
                <map:act type="cmeRedirectAction">
                    <map:parameter name="emrid" value="${emrid}"/>
                    <map:parameter name="host" value="${host}"/>
                    <map:parameter name="query-string" value="${query-string}"/>
                    <map:redirect-to uri="{cme-redirect}"/>
                </map:act>
            </map:match>
            <map:match pattern="cookiesFetch.html" type="wildcard">
                <map:generate type="file" src="${live-base}/content/cookiesFetch.html"/>
                <map:transform type="saxon" src="context://xsl/cookiesFetch.xsl">
                    <map:parameter name="sunetid" value="${sunetid}"/>
                    <map:parameter name="ticket" value="${ticket}"/>
                    <map:parameter name="proxy-links" value="${proxy-links}"/>
                    <map:parameter name="path" value="${path}"/>
                    <map:parameter name="entryUrl" value="${entryUrl}"/>
                    <map:parameter name="title" value="${title}"/>
                </map:transform>
                <map:serialize type="xhtml"/>
            </map:match>
            <map:match pattern="postMethodHelper.html" type="wildcard">
                <map:generate type="request"/>
                <map:transform type="saxon" src="context://xsl/postMethodHelper.xsl">
                    <map:parameter name="sunetid" value="${sunetid}"/>
                    <map:parameter name="ticket" value="${ticket}"/>
                    <map:parameter name="proxy-links" value="${proxy-links}"/>
                </map:transform>
                <map:serialize type="xhtml"/>
            </map:match>
            <!-- used for tracking user IPGroup in interfaces external to laneweb (catalog, elane) -->
            <map:match pattern="ipGroupFetch" type="wildcard">
                <map:generate type="request"/>
                <map:transform type="saxon" src="context://xsl/ipGroupFetch.xsl">
                    <map:parameter name="ipgroup" value="${ipgroup}"/>
                    <map:parameter name="callback" value="${callback}"/>
                </map:transform>
                <map:serialize type="text"/>
            </map:match>
            <!-- external resource searches, normally intercepted by javascript -->
            <map:match pattern="^http" type="regex-parameter">
                <map:parameter name="parameter-name" value="source"/>
                <map:act type="form-redirect">
                    <map:parameter name="query" value="${query}"/>
                    <map:parameter name="source" value="${source}"/>
                    <map:redirect-to uri="{form-redirect-key}"/>
                </map:act>
            </map:match>
            <map:match pattern="plain/secure/**.html" type="wildcard">
                <map:generate type="file" src="cocoon:/plain/error.html"/>
                <map:serialize type="xhtml" status-code="403"/>
            </map:match>
            <map:match pattern="plain/**.html" type="wildcard">
                <map:generate type="file" src="cocoon:/content/{1}.html"/>
                <map:transform type="cme-links"/>
                <map:transform type="saxon" src="context://xsl/laneweb.xsl">
                    <map:parameter name="request-uri" value="${request-uri}"/>
                    <map:parameter name="base-path" value="${base-path}"/>
                    <map:parameter name="query-string" value="${query-string}"/>
                    <map:parameter name="proxy-links" value="${proxy-links}"/>
                    <map:parameter name="sunetid" value="${sunetid}"/>
                    <map:parameter name="ipgroup" value="${ipgroup}"/>
                    <map:parameter name="query" value="${query}"/>
                    <map:parameter name="template" value="${template}"/>
                    <map:parameter name="source" value="${source}"/>
                    <map:parameter name="search-tab" value="${searchTab}"/>
                    <map:parameter name="mesh" value="${mesh}"/>
                    <map:parameter name="version" value="${version}"/>
                    <map:parameter name="referrer" value="${referer}"/>
                    <map:parameter name="name" value="${name}"/>
                </map:transform>
                <map:transform type="proxy-links">
                    <map:parameter name="proxy-links" value="${proxy-links}"/>
                    <map:parameter name="base-path" value="${base-path}"/>
                    <map:parameter name="ipgroup" value="${ipgroup}"/>
                </map:transform>
                <map:transform type="namespace-filter"/>
                <map:serialize type="xml"/>
            </map:match>
            <map:match pattern="**.html" type="wildcard">
                <map:aggregate element="doc" label="content">
                    <map:part src="cocoon:/content/{1}.html"/>
                    <map:part src="cocoon:/content/sitemap.html"/>
                    <map:part src="cocoon:/content/templates/${template}.xml"/>
                </map:aggregate>
                <map:transform type="cme-links"/>
                <map:transform type="saxon" src="context://xsl/laneweb.xsl">
                    <map:parameter name="request-uri" value="${request-uri}"/>
                    <map:parameter name="base-path" value="${base-path}"/>
                    <map:parameter name="query-string" value="${query-string}"/>
                    <map:parameter name="proxy-links" value="${proxy-links}"/>
                    <map:parameter name="sunetid" value="${sunetid}"/>
                    <map:parameter name="ipgroup" value="${ipgroup}"/>
                    <map:parameter name="query" value="${query}"/>
                    <map:parameter name="template" value="${template}"/>
                    <map:parameter name="source" value="${source}"/>
                    <map:parameter name="search-tab" value="${searchTab}"/>
                    <map:parameter name="mesh" value="${mesh}"/>
                    <map:parameter name="version" value="${version}"/>
                    <map:parameter name="referrer" value="${referer}"/>
                    <map:parameter name="name" value="${name}"/>
                </map:transform>
                <map:transform type="proxy-links">
                    <map:parameter name="proxy-links" value="${proxy-links}"/>
                    <!-- use this base-path to redirect proxy auth to lane.stanford.edu -->
                    <!--<map:parameter name="base-path" value="http://lane.stanford.edu"/>-->
                    <map:parameter name="base-path" value="${base-path}"/>
                    <map:parameter name="ipgroup" value="${ipgroup}"/>
                </map:transform>
                <map:transform type="namespace-filter"/>
                <map:serialize type="xhtml"/>
            </map:match>
            <map:match pattern="**.xml" type="wildcard">
                <map:generate type="file" src="${live-base}/content/{1}.xml"/>
                <map:serialize type="xml"/>
            </map:match>
            <map:match pattern="_vti**" type="wildcard">
                <map:generate src="cocoon:/error.html" type="file"/>
                <map:serialize type="xhtml" status-code="404"/>
            </map:match>
            <map:match pattern="MSOffice**" type="wildcard">
                <map:generate src="cocoon:/error.html" type="file"/>
                <map:serialize type="xhtml" status-code="404"/>
            </map:match>
            <map:match pattern="**.*" type="wildcard">
                <map:mount uri-prefix="" check-reload="no" src="static.xmap"/>
            </map:match>
            <map:match pattern="**" type="wildcard">
                <map:redirect-to uri="/{1}/index.html"/>
            </map:match>
            <map:handle-errors>
                <map:aggregate element="doc" label="content">
                    <map:part src="cocoon:/content/error.html"/>
                    <map:part src="cocoon:/content/sitemap.html"/>
                    <map:part src="cocoon:/content/templates/${template}.xml"/>
                </map:aggregate>
                <map:transform type="cme-links"/>
                <map:transform type="saxon" src="context://xsl/laneweb.xsl">
                    <map:parameter name="request-uri" value="${request-uri}"/>
                    <map:parameter name="base-path" value="${base-path}"/>
                    <map:parameter name="query-string" value="${query-string}"/>
                    <map:parameter name="proxy-links" value="${proxy-links}"/>
                    <map:parameter name="sunetid" value="${sunetid}"/>
                    <map:parameter name="ipgroup" value="${ipgroup}"/>
                    <map:parameter name="query" value="${query}"/>
                    <map:parameter name="template" value="${template}"/>
                    <map:parameter name="source" value="${source}"/>
                    <map:parameter name="search-tab" value="${searchTab}"/>
                    <map:parameter name="mesh" value="${mesh}"/>
                    <map:parameter name="version" value="${version}"/>
                    <map:parameter name="referrer" value="${referer}"/>
                    <map:parameter name="name" value="${name}"/>
                </map:transform>
                <map:transform type="proxy-links">
                    <map:parameter name="proxy-links" value="${proxy-links}"/>
                    <map:parameter name="base-path" value="${base-path}"/>
                    <map:parameter name="ipgroup" value="${ipgroup}"/>
                </map:transform>
                <map:transform type="namespace-filter"/>
                <map:serialize type="xhtml" status-code="404"/>
            </map:handle-errors>
        </map:pipeline>
    </map:pipelines>
</map:sitemap>
