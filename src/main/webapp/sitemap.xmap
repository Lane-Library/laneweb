                    <map:parameter name="query-string" value="${request/queryString}"/>
                    <map:parameter name="proxy-links" value="${proxyLinks}"/>
                    <map:parameter name="sunetid" value="${session/attributes/user/sunetId}"/>
                    <map:parameter name="ip-group" value="${session/attributes/user/IPGroup}"/>
                    <map:parameter name="q" value="${request/parameters/q}"/>
                    <map:parameter name="template" value="${request/parameters/template}"/>
                    <map:parameter name="source" value="${request/parameters/source}"/>
                    <map:parameter name="search-tab" value="${request/parameters/searchTab}"/>
                    <map:parameter name="mesh-term" value="${request/parameters/m}"/>
                    <map:parameter name="version" value="${pom.version}"/>
                    <map:parameter name="referrer" value="${request/headers/referer}"/>
                    <map:parameter name="name" value="${session/attributes/user/name}"/>
                </map:transform>
                <map:transform type="proxy-links">
                    <map:parameter name="proxy-links" value="${proxyLinks}"/>
                    <map:parameter name="base-path" value="${request/contextPath}"/>
                    <map:parameter name="sunetid" value="${session/attributes/user/sunetId}"/>
                    <map:parameter name="ticket" value="${session/attributes/user/ticket}"/>
                    <map:parameter name="ip-group" value="${session/attributes/user/IPGroup}"/>
                </map:transform>
                <map:transform type="namespace-filter"/>
                <map:serialize type="xhtml" status-code="404"/>
            </map:handle-errors>-->
        </map:pipeline>
    </map:pipelines>
</map:sitemap>
