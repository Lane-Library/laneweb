            </action>
            <action date="2012-08-07" issue="73359" dev="ceyates" type="fix">
                BeanInstantiationException appearing in logs
            </action>
        </release>
        <release date="2012-08-06" version="1.13.6" description="1.13.6 bug fix release [reverted after deployment]">
