<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://maven.apache.org/changes/1.0.0"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/plugins/maven-changes-plugin/xsd/changes-1.0.0.xsd">
    <properties>
        <author email="ceyates@stanford.edu">Charles Yates</author>
        <title>Laneweb Releases</title>
    </properties>
    <body>
        <release date="2012-07-16" version="1.13.2" description="1.13.2 bug fix release">
            <action date="2012-07-16" issue="72768" dev="ceyates" type="add">
                Add feedback to "Save this search" button
            </action>
            <action date="2012-07-09" issue="72694" dev="ryanmax" type="add">
                track spelling and findit suggestions
            </action>
            <action date="2012-07-09" issue="72686" dev="ryanmax" type="add">
                use google events in addition to log4j to record querymap suggestions
            </action>
            <action date="2012-07-09" issue="72681" dev="ceyates" type="fix">
                url tester fails to report response headers
            </action>
            <action date="2012-07-03" issue="72480" dev="ceyates" type="add">
                create a ScoreStrategy to generate score for ContentResultSearchResult
            </action>
            <action date="2012-07-01" issue="72299" dev="ceyates" type="add">
                simplify domain objects by replacing the XMLizable interface
            </action>
            <action date="2012-06-28" issue="71980" dev="ceyates" type="update">
                handle a null query more gracefully
            </action>
            <action date="2012-06-29" issue="68538" dev="ceyates" type="update">
                upgrade yui to 3.5.1
            </action>
        </release>
        <release date="2012-07-11" version="1.13.1" description="1.13.1 emergency release">
            <action date="2012-07-11" issue="72795" dev="alainb" type="fix">
                persistent login errors
            </action>
        </release>
        <release date="2012-07-09" version="1.13.0" description="1.13.0 feature release">
            <action date="2012-06-25" issue="72075" dev="ceyates" type="update">
                enable bookmarks and login flow features by default 
            </action>
            <action date="2012-06-26" issue="71929" dev="olgary" type="update">
                make anchors.css live - removed the background for links rollovers in the left column
            </action>
            <action date="2012-06-26" issue="71914" dev="olgary" type="fix">
                dragging the bookmark bookmarklet to the toolbar fails in FF 
            </action>
            <action date="2012-06-25" issue="71885" dev="ryanmax" type="update">
                Rename the values in the search drop down 
            </action>
            <action date="2012-06-26" issue="71869" dev="ryanmax" type="fix">
                missing some event tracking data
            </action>
            <action date="2012-06-26" issue="70604" dev="olgary" type="fix">
                right clicking on Bookmark on Lane button in IE fails to enable add to favorites
            </action>
        </release>
        <release date="2011-09-02" version="1.12.0" description="1.12.0 refactor release"/>
        <release date="2010-09-20" version="1.11.0" description="1.11.0 redesign release"/>
    </body>
</document>
