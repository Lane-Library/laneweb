		<script language="VBScript" type="text/vbscript">
		<!-- // Visual basic helper required to detect Flash Player ActiveX control version information
		Function VBGetSwfVer(i)
			on error resume next
			Dim swControl, swVersion
			swVersion = 0
			
			set swControl = CreateObject("ShockwaveFlash.ShockwaveFlash." + CStr(i))
			if (IsObject(swControl)) then
				swVersion = swControl.GetVariable("$version")
			end if
			VBGetSwfVer = swVersion
		End Function
		// -->
		</script>
		
		<script type="text/javascript">
		<!--
			var flashVersion = 0;
			var MSIE = ((navigator.userAgent.indexOf('MSIE') != -1) && (navigator.userAgent.indexOf('Opera') ==-1)) ? true : false;
			//document.write("MSIE: " + MSIE);
			function flashLoad() {
				//array length checks exclude MSIE's empty array implementation
				if (navigator.plugins && navigator.plugins.length && navigator.mimeTypes && navigator.mimeTypes.length) {
					var fp = navigator.plugins["Shockwave Flash"];
					var fm = navigator.mimeTypes["application/x-shockwave-flash"];
					if (fp && fm && fm.enabledPlugin) {
						var d = fp.description;
						if (d) {
							flashVersion = d.charAt(d.indexOf('.')-1);
						}
					}
				} else if (MSIE) {
					var versionStr, tempArray, tempString, versionArray, versionMajor, versionMinor, versionRevision, versionString;
					for (var i=6;i<25;i++) {	
						versionStr = VBGetSwfVer(i);
						if (versionStr != 0) {
							tempArray         = versionStr.split(" ");
							tempString        = tempArray[1];
							versionArray      = tempString.split(",");				
							versionMajor      = versionArray[0];
							versionMinor      = versionArray[1];
							versionRevision   = versionArray[2];
							versionString     = versionMajor + "." + versionRevision;   // 7.0r24 == 7.24
							flashVersion = parseFloat(versionString);
							if (flashVersion >=6) break;
						}
					}
				}
				
				if (flashVersion >= 6) {
					document.getElementById("indexFlashTimeline").style.display="block";
					document.getElementById("indexFlashTimeline").style.visibility="visible";
					document.getElementById("indexCollageBottomPart").style.display="none";
					document.getElementById("indexCollageBottomPart").style.visibility="hidden";
				} else {
					document.getElementById("indexFlashTimeline").style.display="none";
					document.getElementById("indexFlashTimeline").style.visibility="hidden";
					document.getElementById("indexCollageBottomPart").style.display="block";
					document.getElementById("indexCollageBottomPart").style.visibility="visible";
				}
			}
		//-->
		</script>
